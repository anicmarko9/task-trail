{
  "name": "task-trail",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "sitemap": "next-sitemap --config ./sitemap.config.mjs",
    "postbuild": "next-sitemap --config ./sitemap.config.mjs",
    "typecheck": "tsc",
